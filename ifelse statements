import java.util.Scanner;
public class ifelse{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number");
        int n =sc.nextInt();
        if(n>0){
            System.out.println("+ve");
        }else if(n==0){
            System.out.println("neutral");
        }else{
            System.out.println("-ve");
        }
    }
}


